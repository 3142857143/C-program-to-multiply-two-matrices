#include <stdio.h>
#include <conio.h>
#include <math.h>
int main()
{
        int r, c, a[100][100], b[100][100], i, j;
        printf("Enter the number of rows: ");
        scanf("%d", &r);
        printf("Enter the number of colums: ");
        scanf("%d", &c);
        for(i=0; i<r;++i)
        {
                for(j=0;j<c;++j)
                {
                        printf("Enter element a%d%d: ", i+1, j+1);
                        scanf("%d", &a[i][j]);
                        
                }
        }
        for(i=0;i<r;++i)
        {
                for(j=0;j<c;++j)
                {
                        printf("Enter element b%d%d: ", i+1, j+1);
                        scanf("%d", &b[i][j]);
                }
        }
        return 0;
}
